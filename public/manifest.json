{
  "manifest_version": 3,
  "version": "1.0.2",
  "short_name": "React App",
  "name": "오늘 뭐임",
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "24": "icon24.png",
      "32": "icon32.png"
    },
    "default_title": "오늘 뭐임",
    "default_popup": "index.html"
  },
  "permissions": ["storage", "https://github.com/"],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "start_url": ".",
  "display": "standalone",
  "theme_color": "#000000",
  "background_color": "#ffffff"
}
